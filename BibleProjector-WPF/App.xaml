<Application x:Class="BibleProjector_WPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BibleProjector_WPF"
             xmlns:converters="clr-namespace:BibleProjector_WPF.Converter"
             StartupUri="MainWindow.xaml"
             Exit="ProgramExit"
             DispatcherUnhandledException="ExitFromError">
    <Application.Resources>
        <converters:VisibilityConverter x:Key="VisibilityConverter"></converters:VisibilityConverter>
    </Application.Resources>
</Application>
